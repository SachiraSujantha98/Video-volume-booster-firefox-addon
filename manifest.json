{
  "manifest_version": 2,
  "name": "Video Volume Booster",
  "version": "1.0",
  "description": "Boost the volume of any video playing in Firefox",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Volume Booster"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
